if (<%= !@job_application.id.nil? %>) {
  form = document.querySelector(".filter-select")
  if (form) {
    Rails.fire(form, 'submit')
  } else {
    console.log("hello")
    jobsIndex = document.querySelector(".ajax-container")
    superstarsIndex = document.querySelector(".dash-container")
    <% jobs_partial = j render "./jobs/index", jobs: @jobs %>
    <% applications_partial = j render 'shared/applications_dash', applications: @applications %>
    jobsIndex.innerHTML = ''
    jobsIndex.insertAdjacentHTML("beforeend", "<%= jobs_partial %>")
    superstarsIndex.innerHTML = ''
    superstarsIndex.insertAdjacentHTML("beforeend", "<%= applications_partial %>")
  }
}


